{
  "name": "react-joi-forms",
  "version": "2.1.4",
  "description": "React forms from Joi schema",
  "main": "lib/index.js",
  "peerDependencies": {
    "react": ">=0.13.0"
  },
  "keywords": [
    "react",
    "joi",
    "react-component",
    "form"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test-browser": "karma start ./karma.conf.js  --single-run",
    "prebuild": "rm -rf lib && rm -rf build",
    "build": "babel --stage 1 --plugins transform-class-properties ./src --out-dir ./lib",
    "prepublish": "npm run build",
    "dev": "webpack-dev-server --config ./example/server.config.js --hot --port 8881",
    "example": "webpack --config ./example/example.config.js"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.3.0",
    "chai": "^3.2.0",
    "enzyme": "^2.8.0",
    "jest": "^19.0.2",
    "jsdom": "^9.12.0",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "react-json-inspector": "^7.0.0",
    "react-test-renderer": "^15.4.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.14",
    "transfer-webpack-plugin": "^0.1.4",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appersonlabs/react-joi-forms"
  },
  "author": "Matt Apperson",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "joi-browser": "^10.0.6",
    "lodash.isequal": "^4.4.0",
    "material-ui": "^0.17.2",
    "react-tap-event-plugin": "^2.0.1"
  }
}
